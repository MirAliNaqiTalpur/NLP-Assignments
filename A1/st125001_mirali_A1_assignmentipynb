   "cell_type": "code",
   "execution_count": 10,
   "metadata": {},
   "outputs": [],
   "source": [
    "torch.manual_seed(42)\n",
    "voc_size   = len(vocabs)\n",
    "emb_size = batch_size = window_size = 2"
   ]
