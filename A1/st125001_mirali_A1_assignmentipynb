    "# China - Beijing + Baghdad\n",
    "# ground-truth == y_true == 'Iraq'\n",
    "i = 1\n",
    "y_pred = get_embed(neg_embeds, capital[i][1].lower()) - get_embed(neg_embeds, capital[i][0].lower()) + get_embed(neg_embeds, capital[i][2].lower())\n",
    "y_pred"
   ]
